<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="zywork-ucenter-consumer" owner="top.zywork" organization="top.zywork"/>
    <dubbo:registry address="zookeeper://localhost:2181"/>

    <dubbo:reference id="userService" interface="top.zywork.service.UserService"/>
    <dubbo:reference id="roleService" interface="top.zywork.service.RoleService"/>
    <dubbo:reference id="permissionService" interface="top.zywork.service.PermissionService"/>

</beans>